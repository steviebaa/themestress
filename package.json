{
  "name": "themestress",
  "author": "Steve Richardson",
  "license": "MIT",
  "scripts": {
    "build": "parcel build src/index.html -d build --no-source-maps",
    "lint": "eslint . --ext .ts,.tsx",
    "lint:fix": "npm run lint -- --fix",
    "test": "jest --coverage  --collectCoverageFrom=src/**/*.{ts,tsx}",
    "test:watch": "jest --coverage --watch",
    "test:clear-cache": "jest --clearCache",
    "start": "parcel src/index.html -d ./build --no-source-maps",
    "prune": "ts-prune -i .spec.ts$",
    "format": "prettier --write \"src/**/*.{ts,js,tsx,jsx}\"",
    "package:build": "parcel build src/index.html -d dist --no-source-maps",
    "package:format": "prettier --write \"src/themestress/**/*.{ts,js,tsx,jsx}\"",
    "package:lint": "eslint ./src/themestress --ext .ts,.tsx",
    "pack:copy-files": "cp ./README.md ./dist/README.md && cp ./src/themestress/template-package.json ./dist/package.json",
    "pack": "npm run package:format && npm run package:lint && cd src/themestress && tsc -p tsconfig.build.json && cd ../.. && npm run pack:copy-files"
  },
  "dependencies": {
    "prismjs": "^1.26.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-router": "^6.2.1",
    "react-router-dom": "^6.2.1",
    "refractor": "^4.4.0"
  },
  "devDependencies": {
    "@babel/core": "^7.16.7",
    "@emotion/jest": "^11.7.1",
    "@emotion/react": "^11.7.1",
    "@emotion/styled": "^11.6.0",
    "@types/enzyme": "^3.10.11",
    "@types/jest": "^27.4.1",
    "@types/node": "^17.0.9",
    "@types/prismjs": "^1.16.6",
    "@types/react": "^17.0.38",
    "@types/react-dom": "^17.0.11",
    "@types/react-router": "^5.1.17",
    "@types/react-router-dom": "^5.3.2",
    "@typescript-eslint/eslint-plugin": "^5.10.0",
    "@typescript-eslint/parser": "^5.10.0",
    "@wojtekmaj/enzyme-adapter-react-17": "^0.6.6",
    "babel-plugin-module-resolver": "^4.1.0",
    "check-node-version": "^4.2.1",
    "enzyme": "^3.11.0",
    "eslint": "^8.7.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.28.0",
    "jest": "^27.4.7",
    "jest-canvas-mock": "^2.3.1",
    "jest-mock-external-components": "^3.0.1",
    "jest-svg-transformer": "^1.0.0",
    "parcel-bundler": "^1.12.5",
    "prettier": "^2.5.1",
    "ts-jest": "^27.1.3",
    "ts-node": "^10.4.0",
    "ttypescript": "^1.5.13",
    "typescript": "^4.6.3"
  }
}
